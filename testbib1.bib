doi = {10.1016/j.jfluidstructs.2004.12.003},
file = {:C$\backslash$:/Users/Jeffrey/Dropbox/Papers/Papers/Bluff Body Aerodynamics/Finned tubes/Ziada 2005 - The effect of fins on vortex shedding from a cylinder in cross-flow.pdf:pdf},
