issn = {08899746},
journal = {Journal of Fluids and Structures},
keywords = {Elliptic trajectory,Orbiting motion,Pivoted cylinder,Vortex-induced vibrations},
pages = {343--358},
publisher = {Elsevier},
title = {{Orbiting response in vortex-induced vibrations of a two-degree-of-freedom pivoted circular cylinder}},
url = {http://dx.doi.org/10.1016/j.jfluidstructs.2011.08.014},
