isbn = {978-0-7918-5451-8},
issn = {08899746},
journal = {Journal of Fluids and Structures},
keywords = {,Acoustic resonance,Coincidence resonance,Finned cylinders,Pre-coincidence resonance,Tandem cylinders,Vortex shedding},
number = {7},
