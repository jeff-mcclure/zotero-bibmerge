	file = {Bakker et al. 2000 - Learning chaotic attractors by neural networks.pdf:D\:\\Sync\\Nextcloud\\3. Literature\\Chaos Theory\\Bakker et al. 2000 - Learning chaotic attractors by neural networks.pdf:application/pdf},
}

